<div class="row">
  <div class="col-md-6">
      
    <h2>Sign Up</h2>

    <form class="form signup-form">
      <div class="form-group">
        <label for="username-input-signup">Username:</label>
        <input class="form-input" type="text" id="username-input-signup" />
      </div>

      <div class="form-group">
        <label for="password-input-signup">Password:</label>
        <input class="form-input" type="password" id="password-input-signup" />
      </div>

      <div class="form-group">
        <button class="btn btn-danger" type="submit">Sign Up</button>
      </div>
    </form>
  </div>

<script src="/javascript/signup.js"></script>